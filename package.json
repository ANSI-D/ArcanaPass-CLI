{
  "name": "arcanapass",
  "version": "1.0.0",
  "description": "A stateless password generator CLI tool - generates secure, deterministic passwords without storing anything locally",
  "main": "cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ANSI-D/ArcanaPass-CLI.git"
  },
  "bin": {
    "arcanapass": "cli.js"
  },
  "scripts": {
    "start": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "cli": "node cli.js",
    "install:frontend": "cd frontend && npm install"
  },
  "keywords": [
    "password",
    "generator",
    "stateless",
    "deterministic",
    "cli",
    "security"
  ],
  "author": "ansi-d",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
